{
  "schemaVersion": 1,
  "id": "serverutils",
  "version": "${version}",
  "name": "ServerUtils",
  "description": "Utilities for a Minecraft Server.",
  "authors": [
    "oskhe"
  ],
  "contact": {
    "repo": "https://github.com/OskHe/ServerUtils"
  },
  "license": "GPL-3.0",
  "icon": "assets/serverutils/icon.png",
  "environment": "server",
  "entrypoints": {
    "client": [
      "com.oskhe.serverutils.client.ServerUtilsClient"
    ],
    "main": [
      "com.oskhe.serverutils.ServerUtils"
    ]
  },
  "mixins": [
    "serverutils.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
